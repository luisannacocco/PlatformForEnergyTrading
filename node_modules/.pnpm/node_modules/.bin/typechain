#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules/typechain/dist/cli/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules/typechain/dist/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules/typechain/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules/typechain/dist/cli/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules/typechain/dist/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules/typechain/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/typechain@3.0.0_typescript@4.9.5/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typechain/dist/cli/cli.js" "$@"
else
  exec node  "$basedir/../typechain/dist/cli/cli.js" "$@"
fi
