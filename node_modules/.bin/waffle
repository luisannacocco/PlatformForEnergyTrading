#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules/ethereum-waffle/bin/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules/ethereum-waffle/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules/ethereum-waffle/bin/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules/ethereum-waffle/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules/ethereum-waffle/bin/waffle" "$@"
else
  exec node  "$basedir/../.pnpm/ethereum-waffle@3.4.4_bufferutil@4.0.9_encoding@0.1.13_typescript@4.9.5_utf-8-validate@5.0.10/node_modules/ethereum-waffle/bin/waffle" "$@"
fi
