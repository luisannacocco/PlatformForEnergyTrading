  export NODE_PATH="/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_ts-node@9.1.1_typescript@4.9.5__typescript@4.9.5_utf-8-validate@5.0.10/node_modules/hardhat/internal/cli/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_ts-node@9.1.1_typescript@4.9.5__typescript@4.9.5_utf-8-validate@5.0.10/node_modules/hardhat/internal/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_ts-node@9.1.1_typescript@4.9.5__typescript@4.9.5_utf-8-validate@5.0.10/node_modules/hardhat/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_ts-node@9.1.1_typescript@4.9.5__typescript@4.9.5_utf-8-validate@5.0.10/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node_modules:$NODE_PATH"
