#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node-hid@1.3.0/node_modules/node-hid/src/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node-hid@1.3.0/node_modules/node-hid/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node-hid@1.3.0/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node-hid@1.3.0/node_modules/node-hid/src/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node-hid@1.3.0/node_modules/node-hid/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node-hid@1.3.0/node_modules:/home/luisanna/Desktop/NEST CODE/NESTmarketplaceNoIPFSmasca/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node-hid@1.3.0/node_modules/node-hid/src/show-devices.js" "$@"
else
  exec node  "$basedir/../../../../../../node-hid@1.3.0/node_modules/node-hid/src/show-devices.js" "$@"
fi
